{
  "facebook": {
    "facebook_post_container": {
      "selector": "div[role='article']",
      "fallbackSelectors": [],
      "classId": 0,
      "description": "Facebook post container"
    },
    "facebook_profile_picture": {
      "selector": "div[role='article'] img[referrerpolicy='origin-when-cross-origin']",
      "fallbackSelectors": [
        "div[role='article'] a[role='link'] img[alt]",
        "div[role='article'] image"
      ],
      "classId": 1,
      "description": "User profile picture"
    },
    "facebook_username": {
      "selector": "div[role='article'] h2 strong a",
      "fallbackSelectors": [
        "div[role='article'] a[role='link'] strong",
        "div[role='article'] strong span"
      ],
      "classId": 2,
      "description": "Username or page name"
    },
    "facebook_verified_badge": {
      "selector": "div[role='article'] svg[aria-label*='Verified']",
      "fallbackSelectors": [
        "svg[aria-label*='verified']"
      ],
      "classId": 3,
      "description": "Verified badge"
    },
    "facebook_timestamp": {
      "selector": "div[role='article'] a[aria-label*='ago']",
      "fallbackSelectors": [
        "div[role='article'] span[id] a"
      ],
      "classId": 4,
      "description": "Post timestamp"
    },
    "facebook_post_text": {
      "selector": "div[role='article'] div[data-ad-preview='message']",
      "fallbackSelectors": [
        "div[role='article'] div[dir='auto'][style*='text-align']",
        "div[role='article'] div[class*='text']"
      ],
      "classId": 5,
      "description": "Post text content"
    },
    "facebook_post_image": {
      "selector": "div[role='article'] img[data-visualcompletion='media-vc-image']",
      "fallbackSelectors": [
        "div[role='article'] div[role='presentation'] img"
      ],
      "classId": 6,
      "description": "Post image"
    },
    "facebook_post_video": {
      "selector": "div[role='article'] div[role='presentation'] video",
      "fallbackSelectors": [
        "div[role='article'] video"
      ],
      "classId": 7,
      "description": "Post video"
    },
    "facebook_like_button": {
      "selector": "div[role='article'] div[aria-label*='Like']",
      "fallbackSelectors": [
        "div[role='article'] div[role='button'][aria-label*='Like']"
      ],
      "classId": 8,
      "description": "Like button"
    },
    "facebook_comment_button": {
      "selector": "div[role='article'] div[aria-label*='Comment']",
      "fallbackSelectors": [
        "div[role='article'] div[role='button'][aria-label*='Comment']"
      ],
      "classId": 9,
      "description": "Comment button"
    },
    "facebook_share_button": {
      "selector": "div[role='article'] div[aria-label*='Share']",
      "fallbackSelectors": [
        "div[role='article'] div[role='button'][aria-label*='Share']",
        "div[role='article'] div[aria-label*='Send']"
      ],
      "classId": 10,
      "description": "Share button"
    },
    "facebook_like_count": {
      "selector": "div[role='article'] div[role='toolbar'] span[aria-hidden='true']",
      "fallbackSelectors": [
        "div[role='article'] span[aria-label*='reactions']"
      ],
      "classId": 11,
      "description": "Like/reaction count"
    },
    "facebook_comment_count": {
      "selector": "div[role='article'] span[aria-label*='comment']",
      "fallbackSelectors": [],
      "classId": 12,
      "description": "Comment count"
    },
    "facebook_share_count": {
      "selector": "div[role='article'] span[aria-label*='share']",
      "fallbackSelectors": [],
      "classId": 13,
      "description": "Share count"
    }
  },
  "twitter": {
    "twitter_tweet_container": {
      "selector": "[data-testid='tweet']",
      "fallbackSelectors": ["article[role='article']"],
      "classId": 0,
      "description": "Full tweet container"
    },
    "twitter_profile_picture": {
      "selector": "a[role='link'] img[alt]",
      "fallbackSelectors": [
        "[data-testid='Tweet-User-Avatar'] img",
        "img[src*='profile_images']",
        "div[data-testid='Tweet-User-Avatar'] img",
        "a[href*='/photo'] img:first-of-type"
      ],
      "classId": 1,
      "description": "User profile picture"
    },
    "twitter_username": {
      "selector": "[data-testid='User-Name'] span",
      "fallbackSelectors": ["[dir='ltr'] > span"],
      "classId": 2,
      "description": "Username text"
    },
    "twitter_user_id": {
      "selector": "[data-testid='User-Name'] a[href*='/']",
      "fallbackSelectors": [],
      "classId": 3,
      "description": "User ID / handle"
    },
    "twitter_like_button": {
      "selector": "[data-testid='like']",
      "fallbackSelectors": ["button[aria-label*='Like']"],
      "classId": 4,
      "description": "Like button"
    },
    "twitter_comment_button": {
      "selector": "[data-testid='reply']",
      "fallbackSelectors": ["button[aria-label*='Reply']"],
      "classId": 5,
      "description": "Reply button"
    },
    "twitter_repost_button": {
      "selector": "[data-testid='retweet']",
      "fallbackSelectors": ["button[aria-label*='Repost']"],
      "classId": 6,
      "description": "Retweet button"
    },
    "twitter_share_button": {
      "selector": "button[aria-label='Share post']",
      "fallbackSelectors": ["[aria-label*='Share']"],
      "classId": 7,
      "description": "Share button"
    },
    "twitter_bookmark_button": {
      "selector": "[data-testid='bookmark']",
      "fallbackSelectors": ["button[aria-label*='Bookmark']"],
      "classId": 8,
      "description": "Bookmark button"
    },
    "twitter_view_count": {
      "selector": "a[href*='/analytics'] span",
      "fallbackSelectors": [
        "[aria-label*='views'] span",
        "a[href$='/analytics'] span"
      ],
      "classId": 9,
      "description": "View count number"
    },
    "twitter_like_count": {
      "selector": "[data-testid='like'] span",
      "fallbackSelectors": [],
      "classId": 10,
      "description": "Like count number"
    },
    "twitter_comment_count": {
      "selector": "[data-testid='reply'] span",
      "fallbackSelectors": [],
      "classId": 11,
      "description": "Comment/reply count"
    },
    "twitter_repost_count": {
      "selector": "[data-testid='retweet'] span",
      "fallbackSelectors": [],
      "classId": 12,
      "description": "Repost count"
    },
    "twitter_post_text": {
      "selector": "[data-testid='tweetText']",
      "fallbackSelectors": ["[lang]"],
      "classId": 13,
      "description": "Tweet text content"
    },
    "twitter_post_image": {
      "selector": "[data-testid='tweetPhoto'] img",
      "fallbackSelectors": ["img[alt='Image']"],
      "classId": 14,
      "description": "Tweet image"
    },
    "twitter_post_video": {
      "selector": "[data-testid='videoPlayer']",
      "fallbackSelectors": ["[data-testid='videoComponent']", "video"],
      "classId": 15,
      "description": "Tweet video player"
    },
    "twitter_timestamp": {
      "selector": "time",
      "fallbackSelectors": [],
      "classId": 16,
      "description": "Tweet timestamp"
    },
    "twitter_verified_badge": {
      "selector": "[data-testid='icon-verified']",
      "fallbackSelectors": ["svg[aria-label*='Verified']"],
      "classId": 17,
      "description": "Verified checkmark badge"
    }
  }
}
