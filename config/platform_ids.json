{
  "twitter": {
    "twitter_tweet_container": {
      "selector": "[data-testid='tweet']",
      "fallbackSelectors": ["article[role='article']"],
      "classId": 0,
      "description": "Full tweet container"
    },
    "twitter_profile_picture": {
      "selector": "a[role='link'] img[alt]",
      "fallbackSelectors": [
        "[data-testid='Tweet-User-Avatar'] img",
        "img[src*='profile_images']",
        "div[data-testid='Tweet-User-Avatar'] img",
        "a[href*='/photo'] img:first-of-type"
      ],
      "classId": 1,
      "description": "User profile picture"
    },
    "twitter_username": {
      "selector": "[data-testid='User-Name'] span",
      "fallbackSelectors": ["[dir='ltr'] > span"],
      "classId": 2,
      "description": "Username text"
    },
    "twitter_user_id": {
      "selector": "[data-testid='User-Name'] a[href*='/']",
      "fallbackSelectors": [],
      "classId": 3,
      "description": "User ID / handle"
    },
    "twitter_like_button": {
      "selector": "[data-testid='like']",
      "fallbackSelectors": ["button[aria-label*='Like']"],
      "classId": 4,
      "description": "Like button"
    },
    "twitter_comment_button": {
      "selector": "[data-testid='reply']",
      "fallbackSelectors": ["button[aria-label*='Reply']"],
      "classId": 5,
      "description": "Reply button"
    },
    "twitter_repost_button": {
      "selector": "[data-testid='retweet']",
      "fallbackSelectors": ["button[aria-label*='Repost']"],
      "classId": 6,
      "description": "Retweet button"
    },
    "twitter_share_button": {
      "selector": "button[aria-label='Share post']",
      "fallbackSelectors": ["[aria-label*='Share']"],
      "classId": 7,
      "description": "Share button"
    },
    "twitter_bookmark_button": {
      "selector": "[data-testid='bookmark']",
      "fallbackSelectors": ["button[aria-label*='Bookmark']"],
      "classId": 8,
      "description": "Bookmark button"
    },
    "twitter_view_count": {
      "selector": "a[href*='/analytics'] span",
      "fallbackSelectors": [
        "[aria-label*='views'] span",
        "a[href$='/analytics'] span"
      ],
      "classId": 9,
      "description": "View count number"
    },
    "twitter_like_count": {
      "selector": "[data-testid='like'] span",
      "fallbackSelectors": [],
      "classId": 10,
      "description": "Like count number"
    },
    "twitter_comment_count": {
      "selector": "[data-testid='reply'] span",
      "fallbackSelectors": [],
      "classId": 11,
      "description": "Comment/reply count"
    },
    "twitter_repost_count": {
      "selector": "[data-testid='retweet'] span",
      "fallbackSelectors": [],
      "classId": 12,
      "description": "Repost count"
    },
    "twitter_post_text": {
      "selector": "[data-testid='tweetText']",
      "fallbackSelectors": ["[lang]"],
      "classId": 13,
      "description": "Tweet text content"
    },
    "twitter_post_image": {
      "selector": "[data-testid='tweetPhoto'] img",
      "fallbackSelectors": ["img[alt='Image']"],
      "classId": 14,
      "description": "Tweet image"
    },
    "twitter_post_video": {
      "selector": "[data-testid='videoPlayer']",
      "fallbackSelectors": ["[data-testid='videoComponent']", "video"],
      "classId": 15,
      "description": "Tweet video player"
    },
    "twitter_timestamp": {
      "selector": "time",
      "fallbackSelectors": [],
      "classId": 16,
      "description": "Tweet timestamp"
    },
    "twitter_verified_badge": {
      "selector": "[data-testid='icon-verified']",
      "fallbackSelectors": ["svg[aria-label*='Verified']"],
      "classId": 17,
      "description": "Verified checkmark badge"
    }
  }
}
