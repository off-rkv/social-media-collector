        "a[href*='/photo'] img:first-of-type"
      ],
      "classId": 1,
      "description": "Twitter: User profile picture"
    },
    "twitter_username": {
      "selector": "[data-testid='User-Name'] span",
      "fallbackSelectors": ["[dir='ltr'] > span"],
      "classId": 2,
      "description": "Twitter: Username text"
    },
    "twitter_user_id": {
      "selector": "[data-testid='User-Name'] a[href*='/']",
      "fallbackSelectors": [],
